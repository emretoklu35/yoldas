# Podfile generated by Flutter
platform :ios, '12.0'

# CocoaPods analytics sends network stats synchronously affecting flutter build latency.
ENV['COCOAPODS_DISABLE_STATS'] = 'true'

project 'Runner', {
  'Debug' => :debug,
  'Profile' => :release,
  'Release' => :release,
}

def flutter_install_all_ios_pods(file_symlink_strategy = nil)
  require File.expand_path(File.join('packages', 'flutter_tools', 'bin', 'podhelper'), flutter_root)
  flutter_install_ios_engine_pod(file_symlink_strategy)
  flutter_install_pods(file_symlink_strategy)
end

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  flutter_root = File.expand_path('..', __dir__)
  flutter_install_all_ios_pods
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    flutter_additional_ios_build_settings(target)
  end
end
